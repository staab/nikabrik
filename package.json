{
  "name": "nikabrik",
  "version": "0.0.6",
  "description": "A utility for building DVMs",
  "author": "hodlbod",
  "license": "MIT",
  "keywords": [
    "nostr"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/coracle-social/nikabrik.git"
  },
  "types": "./build/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/src/index.mjs",
      "require": "./build/src/index.cjs"
    },
    "./types": "./build/src/index.d.ts"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "pub": "npm i && npm run lint && tsc-multi && npm publish",
    "build": "tsc-multi",
    "clean": "gts clean",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "dependencies": {
    "@coracle.social/lib": "^0.0.4",
    "@coracle.social/network": "^0.0.7",
    "@coracle.social/util": "^0.0.6",
    "dotenv": "^16.3.1",
    "fuse.js": "^7.0.0",
    "hurdak": "^0.2.6",
    "navigatr": "^0.0.3",
    "nostr-tools": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "20.4.6",
    "gts": "^5.0.1",
    "tsc-multi": "^1.1.0",
    "typescript": "~5.1.6"
  }
}
